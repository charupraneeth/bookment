<div>
    {{#each books}}
            <div class="card my-2 border-secondary">
                <div class="card-header h2">
                    Title : {{title}}
                </div>
                <div class="card-body">
                    <h5 class="card-title">Author : {{author}}</h5>
                    <p class="card-text">isbn : {{isbn}}</p>
                    <a href="/search/{{isbn}}" class="btn btn-primary">Go to reviews</a>
                </div>
            </div>
    {{/each}}
</div>